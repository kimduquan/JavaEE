<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
    <factory>
		<exception-handler-factory>epf.webapp.util.ExceptionUtil</exception-handler-factory>
		<!-- <flow-handler-factory>epf.webapp.workflow.WorkflowFactory</flow-handler-factory> -->
	</factory>
	<flow-definition id="workflow">
		<flow-return id="end">
			<from-outcome>end</from-outcome>
		</flow-return>
		<method-call>
			<method>#{workflow.start}</method>
			<default-outcome>end</default-outcome>
		</method-call>
		<inbound-parameter>
			<name>name</name>
			<value>#{workflow.workflow}</value>
		</inbound-parameter>
	</flow-definition>
</faces-config>
